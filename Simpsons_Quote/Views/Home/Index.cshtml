@{
    ViewData["Title"] = "Home Page";
}

<div id="overall">
    <div id="the_simpsons">
        <img src="~/images/the_simpsons.png" width="500px" />
    </div>
    <div id="div_homer">
        <img src="~/images/homer.png" width="300px" />
    </div>
    <div id="div_bart">
        <img src="~/images/bart.png" width="200px" />
    </div>
    <div id="div_button">
        <a id="button" onclick="myFunction()" class="zoom">
            <img src="~/images/donut.png" width="50px" class="zoom"/>
        </a>
        <p>Hit Me!</p>
    </div>
    <div id="api">

        <div id="lanza">
            <img src="~/images/lanza2.png" id="image_lanza" width="450px" />
        </div>
        <div id="picture">
            <img id="generated_image" height="300px" />
        </div>
        <div>
            <h3 id="name"></h3>
        </div>
        <div>
            <h5 id="quote"></h5>
        </div>
        <div id="div_video">
            <iframe id="video" width="560" height="315" src="" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
    </div>
</div>

<!-- scripts -->
<script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
<script src="js/site.js"></script>
<script src="https://apis.google.com/js/client.js?onload=init"></script>
